generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Service {
  id          Int      @id @default(autoincrement())
  name        String
  image       String
  description String
  serviceCode Int
  isActive    Boolean  @default(true)
  createdAt   DateTime @default(now())
}